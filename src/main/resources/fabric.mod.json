{
	"schemaVersion": 1,
	"id": "limlib",
	"version": "${version}",
	"name": "Liminal Library",
	"description": "A library for liminal mods",
	"authors": [
		"LudoCrypt (Developer)"
	],
	"contact": {
		"homepage": "https://github.com/LudoCrypt/Liminal-Library/wiki",
		"issues": "https://github.com/northernlimit/Liminal-Library-Fabric/issues",
		"sources": "https://github.com/northernlimit/Liminal-Library-Fabric"
	},
	"license": "LGPLv3.0",
	"icon": "assets/limlib/icon.png",
	"environment": "*",
	"mixins": [
		"limlib.mixins.json",
		"limlib.debug.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21.3",
		"java": ">=21",
		"fabric-api": "*"
	},
	"accessWidener": "limlib.accesswidener",
	"custom": {
		"modmenu": {
			"badges": [ "library" ]
		}
	}
}